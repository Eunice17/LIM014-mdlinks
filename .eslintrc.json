{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": "off",
        "arrow-body-style": "off",
        "no-await-in-loop": "off",
        "no-restricted-syntax": "off",
        "quote-props": "off",
        "no-param-reassign": "off"
    }
}